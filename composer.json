{
  "name": "crudch/framework",
  "description": "Small crutch framework for hobby.",
  "keywords": [
    "Framework"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Skopin Alexey",
      "email": "skiphog@mail.ru"
    }
  ],
  "require": {
    "php": "^7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^7"
  },
  "autoload": {
    "psr-4": {
      "Crudch\\": "src/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
